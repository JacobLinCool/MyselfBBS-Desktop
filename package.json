{
    "private": true,
    "name": "myselfbbs-desktop",
    "description": "Myself Desktop (Unofficial)",
    "version": "1.0.0-pre.3",
    "main": "dist/app/index.js",
    "scripts": {
        "start": "cross-env DEV=true concurrently 'vite' 'tsc -w' 'electron-forge start'",
        "build": "vite build && tsc && electron-forge make",
        "package": "electron-forge package",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts .",
        "format": "prettier --write 'src/**/*.{ts,html,vue}'"
    },
    "keywords": [],
    "author": {
        "name": "JacobLinCool",
        "email": "hi@jacoblin.cool",
        "url": "https://jacoblin.cool"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./icon/MyselfBBS"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-dmg",
                    "config": {
                        "format": "ULFO",
                        "icon": "./icon/MyselfBBS.icns"
                    }
                },
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "myselfbbs_desktop",
                        "setupIcon": "./icon/MyselfBBS.ico"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip"
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@jacoblincool/puddle": "^0.1.1",
        "commander": "^9.3.0",
        "electron-reload": "^2.0.0-alpha.1",
        "electron-squirrel-startup": "^1.0.0",
        "express": "^4.18.1",
        "file-mapping": "^0.1.4",
        "fix-path": "^3.0.0",
        "fs-base": "^0.1.1",
        "fuse.js": "^6.6.2",
        "node-fetch": "^2.6.7",
        "pangu": "^4.0.7",
        "video.js": "^7.19.2",
        "videojs-sprite-thumbnails": "^0.6.0",
        "vue": "^3.2.37",
        "vue-router": "^4.0.15",
        "vue-starport": "^0.2.11"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-dmg": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@iconify-json/ic": "^1.1.4",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.41",
        "@types/node-fetch": "^2.6.1",
        "@types/pangu": "^3.3.0",
        "@types/video.js": "^7.3.40",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/compiler-sfc": "^3.2.37",
        "concurrently": "^7.2.1",
        "cross-env": "^7.0.3",
        "electron": "^18.3.3",
        "eslint": "^8.17.0",
        "eslint-plugin-import": "^2.26.0",
        "prettier": "^2.6.2",
        "prettier-plugin-tailwindcss": "^0.1.11",
        "typescript": "^4.7.3",
        "unplugin-icons": "^0.14.3",
        "vite": "^2.9.10",
        "vite-plugin-windicss": "^1.8.4",
        "vue-tsc": "^0.34.17",
        "windicss": "^3.5.4"
    }
}
